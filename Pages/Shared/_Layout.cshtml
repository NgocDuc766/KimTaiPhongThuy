<body>
    <header style="
    background-color: darkred;
">
        <nav class="secondary-nav border-top border-bottom">
            <div class="">
                <div class="row">
                    <div class="top-menu d-flex flex-wrap justify-content-between">
                        <div class="left-block d-flex flex-wrap list-unstyled justify-content-start">
                            <li class="menu-item">
                                <form class="searchbar">
                                    <input type="text" name="search" placeholder="Search Here..">
                                    <i class="icon icon-search"></i>
                                </form>
                            </li>
                        </div>
                        <div class="right-block d-flex flex-wrap justify-content-center">
                            <div class="main-logo">
                                <a href="index">
                                    <img src="~/images/WEB/main-logo.png" alt="logo" style="height:40px; width:100%">
                                </a>
                            </div>
                        </div>
                        <ul class="right-block d-flex flex-wrap list-unstyled justify-content-end">
                            @if (Context.Session.GetString("ErrorMessage") != null)
                            {
                                <div class="alert alert-warning" style="position: absolute; top: 20px; right: 20px; z-index: 999;">
                                    @Context.Session.GetString("ErrorMessage")
                                </div>
                                @* After displaying the message, remove it from the session *@
                                Context.Session.Remove("ErrorMessage");
                            }

                            <li class="menu-item">
                                @if (Context.Session.GetString("UserName") != null)
                                {
                                    <a href="/Order"><i class="icon icon-shopping-cart" style="color: yellow;"></i></a>
                                    <a href="Authentication/Profile" style="color: yellow;">
                                        <i class="icon icon-user"></i>@Context.Session.GetString("UserName")
                                    </a>

                                    @* Logout Button - Use a link to the Logout page *@
                                    <a href="/Authentication/Logout" style="background: none; border: none; color: yellow; cursor: pointer;">
                                        <i class="icon icon-logout"></i> Logout
                                    </a>
                                }
                                else
                                {
                                    <a href="/Authentication/Account" style="color: yellow;">
                                        <i class="icon icon-user"></i>Đăng nhập
                                    </a>
                                }
                            </li>
                        </ul>





                    </div>
                </div>
            </div>
        </nav>
